<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="UserSetInfoCell">
        <attr name="user_info_title" format="string|reference" />
        <attr name="user_info_content" format="string|reference" />
        <attr name="user_info_content_color" format="color|reference" />
        <attr name="user_info_show_icon" format="enum">
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
            <enum name="visible" value="0" />
        </attr>
        <attr name="user_info_edit_clickable" format="boolean" />
        <attr name="user_info_edit_hint" format="string|reference" />

    </declare-styleable>
    <declare-styleable name="TextDrawable">
        <attr name="leftDrawable" format="reference" />
        <attr name="leftDrawableWidth" format="dimension" />
        <attr name="leftDrawableHeight" format="dimension" />
        <attr name="rightDrawable" format="reference" />
        <attr name="rightDrawableWidth" format="dimension" />
        <attr name="rightDrawableHeight" format="dimension" />
        <attr name="topDrawable" format="reference" />
        <attr name="topDrawableWidth" format="dimension" />
        <attr name="topDrawableHeight" format="dimension" />
    </declare-styleable>
</resources>